.tr-headLine{text-align: center;vertical-align: middle;background-color:#ebebeb;}
.tr-targetHover:hover{background-color:#ededed;}
.tr-pickTarget{background-color:#212f52;color:white;font-weight:bold;}
.tr-pickTarget a{color:yellow;font-weight: bold;}


