@Service
public class AcadScheduleBoardServiceImpl implements AcadScheduleBoardService{
	
	@Inject
	private AcadScheduleBoardDAO dao;

	@Override
	public List<CalendarVO> getAllSchedule(String acadscDiv) {
		List<CalendarVO> scheduleList = dao.acadScheduleBoardList(acadscDiv);
		return scheduleList;
	}	
	
	

